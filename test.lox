fun fib(n) {
    if (n < 2) {
        return n;
    }

    return fib(n - 2) + fib(n - 1);
}

fun for_loop(n) {
    for (var x = n; x > 0; x = x - 1) {
        print x;
    }
}

fun while_loop(n) {
    while (n > 0) {
        print n;
        n = n - 1;
    }
}

